SESSION_DRIVER=file
QUEUE_CONNECTION=database

# ── Broadcasting: Pusher (NOT Reverb — Reverb needs a persistent process) ─────
# Sign up free at https://pusher.com → Create App → copy keys below
BROADCAST_CONNECTION=pusher

PUSHER_APP_ID=2116393                  # ← from Pusher dashboard
PUSHER_APP_KEY=a5141559a49c28f899a2            # ← from Pusher dashboard
PUSHER_APP_SECRET=ac7061870c9d1112e9bf            # ← from Pusher dashboard
PUSHER_APP_CLUSTER=ap1                 # ← pick closest region:
                                        #   mt1=US, eu=Europe, ap1=Singapore
                                        #   ap2=Mumbai, ap3=Tokyo, sa1=Brazil

# Remove/clear these so app.js stops trying to use Reverb
REVERB_APP_ID=
REVERB_APP_KEY=
REVERB_APP_SECRET=
REVERB_HOST=
REVERB_PORT=
